{
  "name": "feelink-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "tw": "tailwindcss -i ./src/input.css -o ./src/output.css --min",
    "tw:watch": "tailwindcss -i ./src/input.css -o ./src/output.css --watch",
    "build": "npm run tw && next build",
    "start": "next start"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.13",
    "next": "^14.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13"
  }
}
